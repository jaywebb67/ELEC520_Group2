    {
        "id": "1e8caae564e51d2a",
        "type": "function",
        "z": "508bfdfd6e350e6a",
        "name": "function 8",
        "func": "// Check if msg.payload contains an array of user data\nif (Array.isArray(msg.payload)) {\n    // Create an array to store each individual message\n    const messages = [];\n\n    // Loop through the results and format each as 'user:password'\n    msg.payload.forEach(row => {\n        const formattedPayload = `${row.username}:${row.password}`;\n\n        // Push each formatted message as a new object in the array\n        messages.push({ payload: formattedPayload });\n    });\n\n    // Return the array of messages, one for each user credential\n    return [messages];\n}\n\n// If msg.payload is not an array, return it as is\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
